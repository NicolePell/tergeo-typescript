{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "inlineSourceMap": true,
    "esModuleInterop": true,
    "noEmit": true
  },
  "include": ["./src/**/*"],
  "exclude": ["./src/**/*tests.ts"]
}
